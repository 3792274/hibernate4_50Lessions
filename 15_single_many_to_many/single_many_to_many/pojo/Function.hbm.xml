<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="single_many_to_many.pojo">
	<class name="Function">
		<id name="id">
			<!-- foreign表示引用外键 -->
			<generator class="native"/>
		</id>
		<property name="name"/>
		<property name="code"/>
		<property name="url"/>
	</class>
</hibernate-mapping>